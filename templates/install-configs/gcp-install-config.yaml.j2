apiVersion: v1
baseDomain: # your base domain
compute:
- hyperthreading: Enabled
  name: worker
  platform:
    gcp:
      # 8 cpu 32 GB
      type: "n1-standard-8"
      # update it to the zones you need to be available
      # gcloud compute zones list
      # zones:
      #   - asia-south1-a
      #   - asia-south1-b
  replicas: 5
controlPlane:
  hyperthreading: Enabled
  name: master
  platform:
    gcp:
      # # 4 cpu 16 GB
      # type: "n1-standard-4"
      # update it to the zones you need to be available
      # gcloud compute zones list
      # zones:
      #   - asia-south1-b
      #   - asia-south1-b
  replicas: 3
metadata:
  name: # unique cluster name
networking:
  clusterNetwork:
  - cidr: 10.128.0.0/14
    hostPrefix: 23
  machineCIDR: 10.0.0.0/16
  networkType: OpenShiftSDN
  serviceNetwork:
  - 172.30.0.0/16
platform:
  gcp:
    projectID: # your gcp project id
    region: # gcloud compute regions list
pullSecret: # update your pull secret
sshKey: # update your ssh key
